def check_answers(my_answers,answers):
    """
    Checks the five answers provided to a multiple choice quiz and returns the results.
    """
    results = [False]*len(my_answers)
    print(range(len(my_answers)))
    for index in range(len(my_answers)):
        print(index)
        if my_answers[index] == answers[index]:
            results[index] = True
        else:
            results[index] = False
    return results

def count_answer(results):
    count_correct = 0
    count_incorrect = 0
    for result in results:
        if result == True:
            count_correct += 1
        if result != True:
            count_incorrect += 1
    if count_correct/5 > 0.7:
        print("Congratulations, you passed the test! You scored " + str(count_correct) + " out of 5.")
    elif count_incorrect/5 >= 0.3:
        print("Unfortunately, you did not pass. You scored " + str(count_correct) + " out of 5.")


my_answers = [1, 2, 3, 5, 6]
answers = [1, 2, 3, 5, 6]
results = check_answers(my_answers,answers)
count_answer(results)
